<div class="login-box">
  <div class="login-logo">
     <a href="/"><b>Asen</b>tus</a>
  </div>
  <!-- /.login-logo -->
  <div class="login-box-body">
    <p class="login-box-msg">Sign in to start your session</p>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <form action="/login" method="post">
      <div class="form-group has-feedback">
        <%= f.username_field :email, autofocus: true, autocomplete: "username", class: 'form-control' %>
        <span class="glyphicon glyphicon-user form-control-feedback"></span>
      </div>
      <div class="form-group has-feedback">
       <%= f.password_field :password, autocomplete: "current-password" , class: 'form-control'%>

        <span class="glyphicon glyphicon-lock form-control-feedback"></span>
      </div>
      <div class="row">
        <div class="col-xs-8">
          <% if devise_mapping.rememberable? -%>
            <div class="field">
              <%= f.check_box :remember_me, class: "checkbox i-check" %>
              <%= f.label :remember_me %>
            </div>
          <% end -%>
        </div>
        <!-- /.col -->
        <div class="actions">
        <%= f.submit "Log in", class: 'btn btn-primary btn-block btn-flat' %>
        </div>


<%= render "devise/shared/links" %>
        <!-- /.col -->
      </div>
   <% end %>

  

    <a href="#">I forgot my password</a><br>
    <a href="/signup" id="Register" name="Register" class="text-center">Register a new membership</a>

  </div>
  <!-- /.login-box-body -->
</div>
<!-- /.login-box -->
 

 

 
